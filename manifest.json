{
  "manifest_version": 2,
  "name": "Classic mode for Wikipedia",
  "version": "1.0",
  "browser_specific_settings": {
    "gecko": {
      "strict_min_version": "52.0",      
    }
  },
  "description": "Get the classic Wikipedia interface back, pre-January 2023.",
  "icons": {
    "48": "back.svg"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.wikipedia.org/*"
      ],
      "js": [
        "Classic_mode_Wikipedia.js"
      ]
    }
  ]
}